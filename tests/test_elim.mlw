

module MatchPair

use int.Int

goal g :
  forall x:int.
  match (x,x+1) with (a,b) -> a < b end


end

module DefMatchPair

use int.Int

predicate f (x:int) =
  match (x,x+1) with (a,b) -> a < b end
   
goal g :
  forall x:int. f x

end


module Ref

use int.Int
use ref.Ref

let f (x: ref int) : int
  ensures { result > !x }
  = !x+1

end
