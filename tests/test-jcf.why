


theory Records

  use import list.List
  use import bool.Bool
  use import int.Int

  namespace import R
    type t 'a 'b = {| a : 'a; b : list 'b; |}
  end

  goal g1 :
    let t = {| b = Cons True Nil; a = 1.0; |} in
    match {| t with a = 1 |} with
    | {| R.b = Cons x _ |} -> x = True
    | {| a = a |} -> a = 1
    end

end

