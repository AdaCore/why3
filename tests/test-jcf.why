
(* test file *)

theory Test

  use export int.Int

  use real.Real as R
  logic ( *. ) (x:real) (y:real) : real = R.(*) x y 

  logic power (x : real) (n : int) : real =
     if n = 0 then 1.0 else x *. power x (n-1)

end


