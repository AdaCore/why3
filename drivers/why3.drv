(* Why driver for Why3 syntax *)

printer "why3"
filename "%f-%t-%g.why"

(* transformation "detect_polymorphism" *)

theory BuiltIn
  syntax type  int  "int"
  syntax type  real "real"
  syntax predicate (=)  "(%1 = %2)"
  (* meta "encoding:ignore_polymorphism_ls" predicate (=) *)
end

