printer "why3"
filename "%f-%t-%g.why"

transformations
    "compile_match"
    "eliminate_definition"
    "eliminate_inductive"
    "eliminate_algebraic"
    "eliminate_let"
end

theory BuiltIn
  syntax type   int     "int"
  syntax type   real    "real"
  syntax logic  (_=_)   "(%1 = %2)"
  syntax logic  (_<>_)  "(%1 <> %2)"
end
