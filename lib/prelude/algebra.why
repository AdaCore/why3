theory AC
       type t

       logic op(t,t) : t


       axiom Comm : forall x,y:t. op(x,y) = op(y,x)
       axiom Assoc : forall x,y,z:t. op(op(x,y),z)=op(x,op(y,z))
end

theory Group
       type t
       
       logic neutral : t
       
       logic op(t,t) : t
       logic inv(t) : t
       
       axiom Neutral_def : forall x:t. op(x,neutral) = x

       clone AC with type t = t,
                     logic op = op

       axiom Inv_def : forall x:t. op(x,inv(x)) = neutral
end