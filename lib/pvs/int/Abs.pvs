 BEGIN
  IMPORTING Int
  % do not edit above this line
  
  % Why3 abs1
  abs1(x:int): int = IF (x >= 0) THEN x ELSE (-x) ENDIF
  
  % Why3 abs_le
  abs_le: LEMMA FORALL (x:int, y:int): (abs1(x) <= y) <=> (((-y) <= x) AND
    (x <= y))
  
  % Why3 abs_pos
  abs_pos: LEMMA FORALL (x:int): (abs1(x) >= 0)
  
  
 END Abs
 