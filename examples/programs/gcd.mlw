module M

  use import int.Int
  use import int.Gcd
  use import int.ComputerDivision

  let rec gcd u v variant { v } =
    { u >= 0 /\ v >= 0 }
    if v = 0 then
      u
    else
      gcd v (mod u v)
    { gcd u v result }

end

