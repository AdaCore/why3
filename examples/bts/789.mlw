

module Simple

use int.Int

predicate p int

axiom a : forall n. n >= 1 -> p (n-1) -> p n

meta "test_strategy" predicate p, axiom a

goal g : p 5

end




module Syracure

use int.Int

predicate syr int

axiom syr1 : syr 1

axiom syr2 : forall n. n >= 1 -> syr n -> syr (n+n)

axiom syr3 : forall n. n >= 1 -> syr (n+n+n+1) -> syr n

meta "test_strategy" predicate syr, axiom syr2

goal test: syr ((10+10)+(10+10))

end
