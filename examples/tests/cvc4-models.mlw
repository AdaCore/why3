  function projfl "model_trace:.projfl" (l : list int_type) : int
  =
  match l with
  | Nil -> 0
  | Cons (Integer n) _ -> n
  | _ -> 0
  end

  function projf1 "model_trace:.projf1" (l : int) : bool
  =
  l > 0

  function projf2 "model_trace:.projf2" (l : int) : bool
