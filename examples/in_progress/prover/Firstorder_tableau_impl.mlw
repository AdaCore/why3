         ((update (subst_id_symbol: func (int) (symbol (int))) x
             u.model_symbol_field))
         ((subst_id_fo_term: func (int) (fo_term (int) (int)))))
      ((const (Var_fo_term (-1))));
    let res =
      {
        nlrepr_tableau_field =
