
(* test file *)

theory A
  use import prelude.Int

  type 'a list = Nil | Cons('a, 'a list)

  inductive even_length(int list) =
  | Even_nil : even_length(Nil)
  | Even_cons: forall x,y:'a, l:'a list. 
               even_length(l) -> even_length(cons(x,cons(y,l)))
end

theory TestPrelude 
  use prelude.Int
  use prelude.List
end


