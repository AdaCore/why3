
(* test file *)

theory A

  type t
  logic c:t

end

theory B

  uses A

  logic d : A.t
  logic p : A.t -> prop

end

theory C
  uses B
  logic e : B.A.t
  axiom test : B.p(B.A.c)
end

theory Int

  type int

end

theory List

  type 'a list

  logic nil  : 'a list

  logic cons : 'a, 'a list -> 'a list

  logic is_nil : 'a list -> prop

  uses Int

  logic length : 'a list -> Int.int

end

theory Eq 

  logic eq : 'a, 'a -> prop

end

theory Set

  type elt

  type t 

  logic in_ : elt, t -> prop

  logic empty : t

  axiom empty_def1 : forall x:elt. not in_(x, empty)

  logic add : elt, t -> t

  uses Eq

  axiom add_def1 : forall x,y:elt. forall s:t.
                   in_(x, add(y, s)) <-> (Eq.eq(x, y) or in_(x, s))

end

theory Test

  uses Eq, List

  axiom a : forall x : 'a. not Eq.eq(List.nil, List.cons(List.nil, List.nil))

end

