  goal Quicksort_po_7:
    (forall m:int.
      (forall n:int.
        (forall a:(farray int).
          ((Int.(<=) m n) ->
           ((Int.(<) m n) ->
            (forall a0:(farray int).
              (forall i:int.
                (forall j:int.
                  (((Int.(<=) m j) and
                    ((Int.(<) j i) and
                     ((Int.(<=) i n) and
                      ((permut a a0 m n) and
                       (exists x:int.
                         ((forall r:int.
                            (((Int.(<=) m r) and (Int.(<=) r j)) ->
                             (Int.(<=) (access a0 r : int) x))) and
                          ((forall r:int.
                             (((Int.(<) j r) and (Int.(<) r i)) ->
                              ((access a0 r : int) = x))) and
                           (forall r:int.
                             (((Int.(<=) i r) and (Int.(<=) r n)) ->
                              (Int.(>=) (access a0 r : int) x)))))))))) ->
                   (((Int.(<=) 0 (Int.(-) n m : int)) and
                     (Int.(<) (Int.(-) j m : int) (Int.(-) n m : int))) ->
                    ((Int.(<=) m j) ->
                     (forall a1:(farray int).
                       (((permut a0 a1 m j) and (sorted_array a1 m j)) ->
                        (((Int.(<=) 0 (Int.(-) n m : int)) and
                          (Int.(<) (Int.(-) n i : int) (Int.(-) n m : int))) ->
                         ((Int.(<=) i n) ->
                          (forall a2:(farray int).
                            (((permut a1 a2 i n) and (sorted_array a2 i n)) ->
                             (sorted_array a2 m n))))))))))))))))))

  goal Quicksort_po_8:
    (forall m:int.
      (forall n:int.
        (forall a:(farray int).
          ((Int.(<=) m n) -> ((Int.(>=) m n) -> (permut a a m n))))))

  goal Quicksort_po_9:
    (forall m:int.
      (forall n:int.
        (forall a:(farray int).
          ((Int.(<=) m n) -> ((Int.(>=) m n) -> (sorted_array a m n))))))


