

module Uf1
	use import array.Array
	use import ref.Ref
	use import ref.Refint
	use import int.Int

	let b(x:int): int
	requires { x >= 3 }
	ensures { result = x * x } =
		let a = make x 0 in
		let i = ref 0 in
		while (!i  < length a) do
		variant { length a - !i }
		incr i;
		a[!i-1] <- !i * !i ;
		done; a[!i-1]
end
