
theory Prelude
  use export int.Int
  use export bool.Bool

  type unit = ()
  logic ignore 'a : unit

  type arrow 'a 'b 

  type  ref 'a
  logic (!) (ref 'a) : 'a

  type  label
  logic at  'a label : 'a
  logic old 'a       : 'a

  type exn

  logic lt_nat (x y:int) = 0 <= y and x < y

end


