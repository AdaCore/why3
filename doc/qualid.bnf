\begin{syntax}
  lalpha ::= "a"-"z" | "_"
  \
  ualpha ::= "A"-"Z"
  \
  alpha ::= lalpha | ualpha
  \
  lident ::= lalpha (alpha | digit | "'")*
  \
  uident ::= ualpha (alpha | digit | "'")*
  \
  ident ::= lident | uident
  \
  lqualid ::= lident | uqualid "." lident ;
  \
  uqualid ::= uident | uqualid "." uident ;%
\end{syntax}
